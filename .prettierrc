{
	"printWidth": 80,
	"tabWidth": 2,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"semi": true,
	"jsxSingleQuote": true,
	"singleQuote": true,
	"useTabs": true,
	"endOfLine": "auto",
	"importOrder": [
		"^react(.*)$",
		"<THIRD_PARTY_MODULES>",
		"./types",
		"^[.](?!.*.(scss|css)$).*$",
		"(.*).(scss|css)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
